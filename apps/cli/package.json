{
  "name": "express-next",
  "version": "0.0.1",
  "description": "Production-grade CLI for Express.js applications",
  "keywords": [
    "express",
    "cli",
    "generator",
    "scaffold",
    "typescript",
    "prisma",
    "mongodb"
  ],
  "author": "Mustak Ahmed Khan",
  "license": "MIT",
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iam-mustak-ak/express-next.git"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "express-next": "./dist/bin.js"
  },
  "scripts": {
    "build": "rsbuild build && tsc -b --force --emitDeclarationOnly",
    "dev": "rsbuild build --watch",
    "start": "node dist/bin.js",
    "lint": "eslint . --max-warnings 0",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "commander": "^14.0.3",
    "envinfo": "^7.21.0",
    "fs-extra": "^11.3.3",
    "picocolors": "^1.1.1",
    "prompts": "^2.4.2",
    "zod": "^4.3.6",
    "@express-next/core": "workspace:*",
    "@express-next/plugin-auth": "workspace:*",
    "@express-next/plugin-database": "workspace:*",
    "@express-next/plugin-swagger": "workspace:*",
    "@express-next/plugin-common": "workspace:*",
    "@express-next/plugin-middleware": "workspace:*",
    "@express-next/plugin-views": "workspace:*",
    "@express-next/plugin-testing": "workspace:*",
    "@express-next/plugin-docker": "workspace:*",
    "@express-next/plugin-ci": "workspace:*",
    "@express-next/plugin-quality": "workspace:*",
    "@express-next/plugin-resource": "workspace:*"
  },
  "devDependencies": {
    "@rsbuild/core": "^1.7.3",
    "@rsbuild/plugin-type-check": "^1.3.3",
    "@types/envinfo": "^7.8.4",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^25.2.1",
    "@types/prompts": "^2.4.9",
    "eslint": "^8.57.1",
    "typescript": "^5.9.3"
  }
}
